<query id="insertAntiaccessAccessip" action="insert">
    <tables>
        <table name="antiaccess_access_ip" />
    </tables>
    <columns>
        <column name="access_srl" var="access_srl" filter="number" />
        <column name="ipaddress" var="ipaddress" notnull="notnull" />
        <column name="limit_count" var="limit_count" filter="number" maxlength="11" />
        <column name="limit_display" var="limit_display" filter="number" maxlength="11" />
        <column name="limit_rss" var="limit_rss" filter="number" maxlength="11" />
        <column name="limit_atom" var="limit_atom" filter="number" maxlength="11" />
        <column name="limit_trackback" var="limit_trackback" filter="number" maxlength="11" />
        <column name="limit_act" var="limit_act" filter="number" maxlength="11" />
        <column name="module_srl" var="module_srl" default="0" filter="number" maxlength="11" />
        <column name="document_srl" var="document_srl" default="0" filter="number" maxlength="11" />
        <column name="act" var="act" default="" maxlength="40" />
        <column name="page" var="page" default="1" filter="number" maxlength="11" />
        <column name="block" var="block" default="N" />
        <column name="occur_count" var="occur_count" default="0" filter="number" maxlength="11" />
        <column name="regdate" var="regdate" default="curdate()" />
        <column name="last_update" var="last_update" default="curdate()" />
    </columns>
</query>