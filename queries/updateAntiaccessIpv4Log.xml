<query id="updateAntiaccessIpv4Log" action="update">
    <tables>
        <table name="antiaccess_ipv4_log" />
    </tables>
    <columns>
        <column name="display" var="display" filter="number" maxlength="11" />
        <column name="rss" var="rss" filter="number" maxlength="11" />
        <column name="atom" var="atom" filter="number" maxlength="11" />
        <column name="trackback" var="trackback" filter="number" maxlength="11" />
        <column name="act" var="act" filter="number" maxlength="11" />
    </columns>
    <conditions>
        <condition operation="equal" column="ipaddress" var="ipaddress" />
    </conditions>
</query>