<query id="getAntiaccessFollowhostList" action="select">
    <tables>
        <table name="antiaccess_follow_host" />
    </tables>
    <columns>
        <column name="*" />
    </columns>
    <conditions>
        <condition operation="equal" column="follow_srl" var="follow_srl" filter="number" pipe="where" />
        <condition operation="equal" column="host" var="host" pipe="and" />
        <condition operation="equal" column="follow_key" var="follow_key" pipe="and" />
        <condition operation="notequal" column="follow_key" var="not_follow_key" pipe="and" />
        <condition operation="equal" column="state" var="state" pipe="and" />
        <condition operation="notin" column="state" var="not_state" pipe="and" />
        <condition operation="equal" column="my_level" var="my_level" filter="number" pipe="and" />
        <condition operation="equal" column="follow_level" var="follow_level" filter="number" pipe="and" />
        <condition operation="equal" column="rank" var="rank" pipe="and" />
        <group pipe="and">
            <condition operation="like" column="host" var="s_host" pipe="or" />
            <condition operation="like" column="state" var="s_state" pipe="or" />
            <condition operation="like" column="my_level" var="s_my_level" pipe="or" />
            <condition operation="like" column="follow_level" var="s_follow_level" pipe="or" />
            <condition operation="like_prefix" column="regdate" var="s_regdate" pipe="or" />
            <condition operation="like_prefix" column="last_update" var="s_last_update" pipe="or" />
            <condition operation="like" column="rank" var="s_rank" pipe="or" />
        </group>
    </conditions>
    <navigation>
        <index var="sort_index" default="follow_srl" order="order_type" />
        <list_count var="list_count" default="20" />
        <page_count var="page_count" default="10" />
        <page var="page" default="1" />
    </navigation>
</query>